  <title>CGV Cinemas Việt Nam  &#8226;  Trang Chủ</title>
</head>
<body>
     <!-- header -->
     <header>
        <nav class="nav-header">
            <a href="/"><img class="logo" src="image/icon-bg.png" alt=""></a>
                <ul class="nav-links">
                    <li class="active"><a data-page="home" href="/" >Trang Chủ</a></li>
                    <li><a data-page="contact"  href="#">Cụm Rạp</a></li>
                    <li><a data-page="projects"  href="#">Lịch Chiếu</a></li>
