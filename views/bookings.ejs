<table class="table table-condensed">
  <thead>
    <tr>
      <th>Movie</th>
      <th>Tickets Purchased</th>
      <th>Movie Showing</th>
    </tr>
  </thead>
  <tbody>
    <tr data-ng-repeat="book in bookings">
      <td>{{book.name}}</td>
      <td>{{book.qty}}</td>
      <td>{{book.date}}</td>
    </tr>
    <tr data-ng-show="bookings.length <= 0">
      <td colspan="3" style="text-align:center;">You have not made any Movie Bookings yet.</td>
    </tr>
  </tbody>
</table>